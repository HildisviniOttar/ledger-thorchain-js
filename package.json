{
  "name": "@thorchain/ledger-thorchain",
  "version": "0.0.3",
  "description": "Node API for THORChain App (Ledger Nano S/X)",
  "main": "./dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thorchain/ledger-thorchain-js.git"
  },
  "keywords": [
    "Ledger",
    "Javascript",
    "Tendermint",
    "THORChain"
  ],
  "author": "THORChain",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/thorchain/ledger-thorchain-js/issues"
  },
  "homepage": "https://github.com/thorchain/ledger-thorchain-js",
  "dependencies": {
    "@ledgerhq/hw-transport": "^6.1.0",
    "bech32": "^2.0.0",
    "ripemd160": "^2.0.2"
  },
  "devDependencies": {
    "@ledgerhq/hw-transport-node-hid": "^6.1.0",
    "@ledgerhq/hw-transport-webhid": "^6.1.0",
    "@ledgerhq/hw-transport-webusb": "^6.1.0",
    "@ledgerhq/logs": "^6.0.2",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@types/ripemd160": "^2.0.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "bip32": "2.0.6",
    "bip39": "3.0.4",
    "core-js": "^3.15.2",
    "crypto-js": "4.0.0",
    "dotenv": "^10.0.0",
    "index.js": "^0.0.3",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "secp256k1": "^4.0.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "scripts": {
    "clean": "rimraf lib/**",
    "build": "yarn clean && rollup -c",
    "test": "jest",
    "lint": "eslint \"{src,__tests__}/**/*.ts\" --fix --max-warnings 0",
    "prepublishOnly": "yarn build",
    "test:unit": "jest -c jest.config.unit.js",
    "test:e2e": "jest -c jest.config.e2e.js --runInBand --detectOpenHandles",
    "serve": "openssl req -nodes -new -x509 -config certs/openssl.cnf -keyout certs/server.key -out certs/server.cert; openssl x509 -in certs/server.cert -out certs/cert.pem -outform PEM; vue-cli-service serve vue_example/main.js"
  },
  "moduleDirectories": [
    "node_modules",
    "dist"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "publishConfig": {
    "access": "public"
  }
}
